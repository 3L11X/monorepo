{
  "name": "@remix-pwa/eslint-config",
  "description": "ESLint config for Remix PWA packages",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:remix-pwa/monorepo",
    "directory": "packages/eslint-packages"
  },
  "engies": {
    "node": ">=18.0.0"
  },
  "main": "node.js",
  "files": [
    "CHANGELOG.md",
    "README.md",
    "src/",
    "node.js"
  ],
  "scripts": {
    "format": "prettier --write 'src/'",
    "lint": "TIMING=1 eslint --fix 'src/'",
    "test": "cross-env jest 'src/' --setupFilesAfterEnv=jest-extended/all --colors --bail --detectOpenHandles --errorOnDeprecated --noStackTrace --forceExit --logHeapUsage --passWithNoTests"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-node": "0.3.9",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-disable": "^2.0.3",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jsdoc": "^46.4.6",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security-node": "^1.1.1",
    "eslint-plugin-sort-destructure-keys": "^1.5.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.2",
    "eslint": "^8.46.0",
    "jest-extended": "^4.0.1",
    "prettier": "^3.0.1",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "eslint": "^8.46.0",
    "typescript": "^5.0.0"
  }
}
